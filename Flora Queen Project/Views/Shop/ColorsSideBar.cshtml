@model IEnumerable<Flora_Queen_Project.Models.Color>

<ul>
    @foreach (var item in Model)
    {
        <li>
            <a class="color-click" data-id="@item.Id">
                @Html.DisplayFor(modelItem => item.Name)
                <span>(@Html.DisplayFor(modelItem => item.Products.Count))</span>
            </a>
        </li>
    }
</ul>
