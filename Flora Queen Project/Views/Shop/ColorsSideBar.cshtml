@model IEnumerable<Flora_Queen_Project.Models.Color>

<ul>
    @foreach (var item in Model)
    {
        <li>
            @* ReSharper disable once Html.IdNotResolved *@
            <a href="#/" class="color-click" data-id="@item.Id">
                <span class="text-muted text-small color-current" data-id="@item.Id">

                </span>
                @Html.DisplayFor(modelItem => item.Name)
                <span>(@Html.DisplayFor(modelItem => item.Products.Count))</span>
            </a>
        </li>
    }
</ul>
