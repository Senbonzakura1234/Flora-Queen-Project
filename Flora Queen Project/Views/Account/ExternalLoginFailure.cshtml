@{
    ViewBag.Title = "Login Failure";
}

<!-- Start Bradcaump area -->
<div class="ht__bradcaump__area bg-image--6">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="bradcaump__inner text-center">
                    <h2 class="bradcaump-title">@ViewBag.Title</h2>
                    <nav class="bradcaump-content">
                        <a class="breadcrumb_item" href="@Url.Action("Index","Home")">Home</a>
                        <span class="brd-separetor">/</span>
                        <span class="breadcrumb_item active">@ViewBag.Title</span>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Bradcaump area -->
<!-- Start My Account Area -->
<section class="my_account_area pt--80 pb--55 bg--white">
    <div class="container">
        <div class="row">
            <div class="mx-auto col-12 alert alert-warning" role="alert">
                <strong>Login Failure</strong> @ViewBag.FailureInfo
            </div>
        </div>
    </div>
</section>
<!-- End My Account Area -->